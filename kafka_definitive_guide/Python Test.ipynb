     ]
    }
   ],
   "source": [
    "producer = KafkaProducer(bootstrap_servers=['kafka1:19092'])\n",
    "future = producer.send('my-topic', b'raw_bytes')\n",
    "try:\n",
    "    record_metadata = future.get(timeout=10)\n",
    "except KafkaError as e:\n",
    "    log.exception(e)\n",
    "    pass\n",
    "\n",
    "print(record_metadata)"
